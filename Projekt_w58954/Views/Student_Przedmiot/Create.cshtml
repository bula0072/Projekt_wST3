@model Projekt_w58954.Student_Przedmiot

@{
    ViewData["Title"] = "Przypisz studenta do przedmiotu";
}
<div class="container">
    <div class="row">
        <div class="col text-center">
            <h1>Przypisz studenta do przedmiotu</h1>
        </div>
    </div>
    <div class="row justify-content-center">
        <div class="col-sm-6">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Student_id" class="control-label">Student</label>
                    <select asp-for="Student_id" class="form-control" asp-items="ViewBag.Student_id"></select>
                </div>
                <div class="form-group">
                    <label asp-for="Przedmiot_id" class="control-label">Przedmiot</label>
                    <select asp-for="Przedmiot_id" class="form-control" asp-items="ViewBag.Przedmiot_id"></select>
                </div>
                <div class="form-group">
                    <input type="submit" value="Przypisz" class="btn btn-light btn-block" />
                    <a class="btn btn-light btn-block" asp-action="Index">Powrót</a>
                </div>
            </form>
        </div>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
