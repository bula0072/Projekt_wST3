@model IEnumerable<Projekt_w58954.Student_Przedmiot>

@{
    ViewData["Title"] = "Lista Studentów i przedmiotów";
}
<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Imię studenta</th>
            <th scope="col">Nazwisko studenta</th>
            <th scope="col">Przedmiot</th>
            <th scope="col">Działanie</th>
        </tr>
    </thead>
    <tbody>

        @{
            int i = 0;
            foreach (var item in Model)
            {
                <tr>
                    <th scope="row">@(i = i + 1)</th>
                    <td>@Html.DisplayFor(modelItem => item.Student.Imie)</td>
                    <td>@Html.DisplayFor(modelItem => item.Student.Nazwisko)</td>
                    <td>@Html.DisplayFor(modelItem => item.Przedmiot.Nazwa_przedmiotu)</td>
                    <td>
                        <a class="btn btn-light" asp-action="Delete" asp-route-id="@item.Id">Wypisz</a>
                    </td>
                </tr>
            }
        }
    </tbody>
</table>
<a class="btn btn-light ml-5" asp-action="Create">Przypisz studenta</a>