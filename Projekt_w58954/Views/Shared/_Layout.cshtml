<!DOCTYPE html>
<html class="bg-secondary">
<head>
    <title>@ViewData["Title"] - Projekt w58954</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body >
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
            <a class="navbar-brand text-light" asp-controller="Home" asp-action="Index">Projekt w58954</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link text-light" asp-area="" asp-controller="Students" asp-action="Index">Studenci</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-light" asp-area="" asp-controller="Przedmiots" asp-action="Index">Przedmioty</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-light" asp-area="" asp-controller="Student_Przedmiot" asp-action="Index">Wykaz</a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <main class=" text-light bg-secondary">
        @RenderBody()
    </main>
    <footer class="border-top footer bg-dark">
        <div class="container text-light">
            &copy; 2020 - Patryk Bukowski
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>